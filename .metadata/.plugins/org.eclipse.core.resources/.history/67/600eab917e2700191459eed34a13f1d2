package com.example.demo.model;

public class Book {
	Integer id;
	String name;

	Book(){
		
	}
	
	public Book(Integer id, String name) {
		this.id = id;
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

}
